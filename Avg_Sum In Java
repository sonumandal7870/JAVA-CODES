import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
    Scanner input = new Scanner(System.in);
        
            int n1,n2,n3;
            n1 = input.nextInt();
            n2 = input.nextInt();
            n3 = input.nextInt();
        
            int avg = (n1+n2+n3)/3;
            System.out.println(avg);
    }
}
